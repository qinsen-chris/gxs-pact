<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gclfax.modules.pact.dao.PactRecordDao" >

  <insert id="save" parameterType="com.gclfax.modules.pact.entity.PactRecordEntity" useGeneratedKeys="true" keyProperty="id" >
    insert into t_pact_record (platform, pact_flag, pact_flag_id,o2m_user_id,pact_path,create_time)
    values (#{platform}, #{pactFlag},#{pactFlagId},#{o2mUserId}, #{pactPath},#{createTime})
  </insert>
  <select id="queryPactPath" resultType="map">
    select pact_path as pactPath
    from t_pact_record
    where platform = #{platform} and pact_flag = #{pactFlag} and pact_flag_id = #{pactFlagId}
  </select>
</mapper>